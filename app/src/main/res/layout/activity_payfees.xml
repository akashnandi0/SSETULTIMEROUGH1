<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_payfees"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/scms1"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.a2j.ssetultimerough.PAYFEES">

    <Button
        android:text="TRANSPORTATION FEE"
        android:textColor="@android:color/holo_red_dark"
        android:textStyle="bold"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:id="@+id/button16"
        android:onClick="tRF"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/button17"
        android:layout_marginTop="27dp" />

    <Button
        android:text="TUTION FEE"
        android:textColor="@android:color/holo_red_dark"
        android:textStyle="bold"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:id="@+id/button17"
        android:onClick="tF"
        android:layout_below="@+id/button16"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="HOSTEL FEE"
        android:textColor="@android:color/holo_red_dark"
        android:textStyle="bold"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:id="@+id/button20"
        android:onClick="hF"
        android:layout_below="@+id/button17"
        android:layout_alignStart="@+id/button17" />

    <Button
        android:text="MESS FEE"
        android:textColor="@android:color/holo_red_dark"
        android:textStyle="bold"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:id="@+id/button19"
        android:onClick="mF"
        android:layout_below="@+id/button20"
        android:layout_alignStart="@+id/button20" />

    <Button
        android:text="LIBRARY FINE"
        android:textColor="@android:color/holo_red_dark"
        android:textStyle="bold"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:id="@+id/button18"
        android:onClick="lF"
        android:layout_below="@+id/button19"
        android:layout_alignStart="@+id/button19" />
</RelativeLayout>
